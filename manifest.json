{
    "manifest_version": 2,
    "name": "Bias Breakdown",
    "description": "Detect bias in news",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
 
    
    "background":{
        "scripts": ["background.js"]
    },
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Bias Breakdown",
        "default_popup": "popup.html"
    }
}